    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.0",
    "react-router-redux": "^4.0.8",
    "react-transform-hmr": "^1.0.4",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
